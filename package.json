{
  "name": "datasetarchiver",
  "version": "1.0.0",
  "description": "microservice to crawl added web ressources",
  "keywords": [
    "archiver",
    "datasetarchiver",
    "crawler",
    "datasets"
  ],
  "author": "Thomas Weber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/websi96/archiver.git"
  },
  "engines": {
    "node": "12.1.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node --max-old-space-size=6144 dist/server/index.js",
    "start:master": "node --max-old-space-size=6144 dist/master/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "node_tls_reject_unauthorized=0 nodemon --watch server server/index.ts | pino-pretty",
    "master": "node_tls_reject_unauthorized=0 nodemon --watch master --watch server master/index.ts | pino-pretty",
    "dev:debug": "nodemon --watch server --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "CRAWL_minRange=0 CRAWL_maxRange=0 MaxFileSizeInBytes=500000 mocha test/**/*.ts --exit",
    "test:debug": "CRAWL_minRange=0 CRAWL_maxRange=0 MaxFileSizeInBytes=500000 mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "file-stream-rotator": "^0.5.5",
    "follow-redirects": "^1.9.0",
    "mime": "^2.4.4",
    "mongoose": "^5.7.1",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "multistream": "^4.0.0",
    "pino": "^5.13.3",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sanitize-filename": "^1.6.3",
    "serve-favicon": "^2.5.0",
    "stream-file-type": "^0.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/mongodb": "^3.3.2",
    "@types/archiver": "^3.0.0",
    "@types/chai": "^4.2.3",
    "@types/content-disposition": "^0.5.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/follow-redirects": "^1.8.0",
    "@types/mime": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.19",
    "@types/mongoose-unique-validator": "^1.0.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.5",
    "@types/pino": "^5.8.10",
    "@types/request-promise-native": "^1.0.16",
    "@types/shelljs": "^0.8.5",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "pino-pretty": "^3.2.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}

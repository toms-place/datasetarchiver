{
  "name": "archiver",
  "version": "0.0.3",
  "description": "crawls added data ressources",
  "keywords": [
    "archiver",
    "crawler",
    "datasets"
  ],
  "author": "Thomas Weber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/websi96/archiver.git"
  },
  "engines": {
    "node": "12.1.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "npm run prod",
    "build": "babel src -d dist/crawler --copy-files",
    "build:master": "babel src/crawl_master.js src/database.js src/config.js -d dist/master & babel src/models/* -d dist/master/models & babel src/services/* -d dist/master/services & babel src/utils/* -d dist/master/utils",
    "server": "babel-node ./src/app.js",
    "server:prod": "node ./dist/crawler/app.js",
    "server:master": "node ./dist/master/crawl_master.js",
    "dev": "npm-run-all server",
    "clean": "rimraf dist",
    "clean:crawler": "rimraf dist/crawlter",
    "clean:master": "rimraf dist/master",
    "prod": "npm-run-all clean build server:prod",
    "test": "mocha --delay --require @babel/register",
    "prod:master": "npm run master",
    "master": "babel-node ./src/crawl_master.js"
  },
  "dependencies": {
    "content-disposition": "^0.5.3",
    "cookie-parser": "~1.4.4",
    "dotenv": "^8.1.0",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "mime": "^2.4.4",
    "mongodb": "^3.3.1",
    "mongoose": "^5.6.11",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "babel-preset-minify": "^0.5.1",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "csv-parser": "^2.3.0"
  }
}

{
  "name": "archiver",
  "version": "0.0.2",
  "description": "crawls added data ressources",
  "keywords": [
    "archiver",
    "crawler",
    "datasets"
  ],
  "author": "Thomas Weber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/websi96/archiver.git"
  },
  "engines": {
    "node": "12.1.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "npm run prod",
    "build": "babel src -d dist --copy-files",
    "server": "babel-node ./src/app.js",
    "server:prod": "DATASETPATH=./data node ./dist/app.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "test": "mocha --require @babel/register"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^3.2.7",
    "mongoose": "^5.5.15",
    "morgan": "~1.9.1",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "npm-run-all": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-pug": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3"
  }
}
